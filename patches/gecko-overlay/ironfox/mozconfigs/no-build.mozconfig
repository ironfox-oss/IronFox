# IronFox "no-build" mozconfig

# If Gecko is built, but not packaged, then disable the compile environment - we don't want to build anything at this point
ac_add_options --disable-compile-environment

# Disable checks for runtime dependencies
## (Needed to prevent failures)
ac_add_options --without-embedded-uniffi-bindgen
ac_add_options --without-nimbus-fml
