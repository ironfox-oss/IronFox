<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<resources xmlns:tools="http://schemas.android.com/tools"
    xmlns:moz="http://mozac.org/tools">
    <!-- Warn users that the browser will restart -->
    <string name="restart_required_summary">Changing this setting will restart your browser</string>

    <!-- Warn users that this setting is not recommended or supported -->
    <string name="unsupported_summary">This setting is NOT recommended or supported. Proceed with caution.</string>

    <!-- IronFox Settings -->
    <string name="if_title" translatable="false">IronFox</string>
    <string name="if_preferences">IronFox settings</string>
    <string name="if_preferences_misc">Miscellaneous</string>
    <string name="if_preferences_privacy">Privacy</string>
    <string name="if_preferences_privacy_security">Privacy and security</string>
    <string name="if_preferences_secret">Secret settings</string>
    <string name="if_preferences_security">Security</string>

    <!-- Accessibility services -->
    <string name="preference_accessibility_enabled">Enable accessibility services</string>

    <!-- Always use private browsing mode -->
    <string name="preference_always_use_private_browsing">Always use private browsing mode</string>

    <!-- Collections -->
    <string name="preference_collections_enabled">Enable collections</string>

    <!-- Cross-origin referer policy -->
    <string name="preference_referer_policy">Cross-origin referer policy</string>
    <string name="preference_referer_policy_summary">Configure the behavior of cross-origin referers</string>
    <string name="preference_referer_policy_always">Always send cross-origin referers (Default)</string>
    <string name="preference_referer_policy_base_domains_match">Send cross-origin referers when base domains match</string>
    <string name="preference_referer_policy_hosts_match">Disable cross-origin referers</string>

    <!-- Disk cache -->
    <string name="preference_cache_enabled">Enable disk cache</string>

    <!-- Firefox Translations -->
    <string name="preference_translations_enabled">Enable Firefox Translations</string>

    <!-- FPP overrides -->
    <string name="preference_fpp_overrides_ironfox_enabled">Enable fingerprinting protection overrides from IronFox</string>
    <string name="preference_fpp_overrides_ironfox_summary">Relaxes fingerprinting protections for certain websites by default to reduce breakage, using a list from IronFox - Changing this setting will restart your browser</string>
    <string name="preference_fpp_overrides_mozilla_enabled">Enable fingerprinting protection overrides from Mozilla</string>
    <string name="preference_fpp_overrides_mozilla_summary">Relaxes fingerprinting protections for certain websites by default to reduce breakage, using a list from Mozilla - Changing this setting will restart your browser</string>

    <!-- IPv6 -->
    <string name="preference_ipv6_enabled">Enable IPv6 network connectivity</string>

    <!-- JavaScript -->
    <string name="preference_javascript_enabled">Enable JavaScript</string>

    <!-- JavaScript JIT -->
    <string name="preference_jit_enabled">Enable JavaScript Just-in-time Compilation (JIT)</string>
    <string name="preference_jit_enabled_caption">Improve JavaScript performance by enabling JIT. (Not recommended for most users)</string>
    <string name="preference_jit_enabled_description">At the cost of security, you can enable JIT to improve performance of websites. Only recommended for users with lower-end devices.</string>
    <string name="preference_jit_enabled_summary">When disabled, performance may be reduced for complex webpages - Changing this setting will restart your browser</string>
    <string name="preference_jit_trusted_principals_enabled">Enable JavaScript Just-in-time Compilation (JIT) for extensions</string>
    <string name="preference_jit_trusted_principals_enabled_summary">When JIT is disabled, re-enables JIT for extensions - Changing this setting will restart your browser</string>

    <!-- Media autoplay-->
    <string name="preference_media_autoplay_title">Media autoplay</string>
    <string name="preference_media_autoplay_description">Control how IronFox blocks media autoplay.</string>
    <string name="preference_autoplay_policy_sticky">Sticky</string>
    <string name="preference_autoplay_policy_transient">Transient (Default)</string>
    <string name="preference_autoplay_policy_click_to_play">Click-to-play</string>

    <!-- PDF.js -->
    <string name="preference_pdfjs_disabled">Open PDF files externally</string>

    <!-- Preferred website appearance -->
    <!-- (The description here is adapted from Firefox for Desktop: https://searchfox.org/mozilla-release/rev/34c2c305/browser/locales/en-US/browser/preferences/preferences.ftl#277) -->
    <string name="preference_website_appearance_title">Preferred website appearance</string>
    <string name="preference_website_appearance_description">Some websites adapt their color scheme based on your preferences. Choose which color scheme you’d like to use for those sites. Note that options other than Light can aid fingerprinting by causing your browser to appear more unique.</string>
    <string name="preference_prefers_browser_color_scheme">Follow browser theme</string>
    <string name="preference_prefers_light_color_scheme">Light (Default)</string>
    <string name="preference_prefers_dark_color_scheme">Dark</string>

    <!-- Printing capabilities -->
    <string name="preference_print_enabled">Enable printing capabilities</string>

    <!-- Safe Browsing -->
    <string name="preference_safe_browsing_enabled">Enable Safe Browsing</string>
    <string name="preference_safe_browsing_enabled_caption">Get real-time protection against dangerous websites, malware, and phishing attempts.</string>
    <string name="preference_safe_browsing_enabled_description">IronFox’s Safe Browsing functionality is currently provided by Google. IronFox will periodically update the database to ensure users are protected against the latest threats, and upon encountering a potential match, IronFox might submit a partial hash of the suspected URL to Google. Note that IronFox proxies these connections, to prevent your public IP address from being associated with these requests.</string>
    <string name="preference_safe_browsing_enabled_summary">Provides real-time protection against dangerous websites, malware, and phishing attempts - Changing this setting will restart your browser</string>

    <!-- Spoof locale to English -->
    <string name="tor_spoof_english_title">Spoof language to en-US</string>
    <string name="tor_spoof_english_caption">Enhance your privacy by masking your device’s language settings.</string>
    <string name="tor_spoof_english_description">Request English (en-US) versions of webpages. This helps your browser appear less unique, strengthening protection against fingerprinting techniques that identify users based on their language preferences.</string>
    <string name="tor_spoof_english">Request English versions of webpages</string>
    <string name="tor_spoof_english_summary">When enabled, strengthens fingerprinting protection</string>

    <!-- Spoof timezone to UTC-0 -->
    <string name="spoof_timezone">Spoof timezone to UTC-0</string>
    <string name="spoof_timezone_summary">When enabled, strengthens fingerprinting protection</string>
    <string name="preference_fpp_overrides_ironfox_timezone_enabled">Enable timezone spoofing overrides from IronFox</string>
    <string name="preference_fpp_overrides_ironfox_timezone_summary">Disables timezone spoofing for certain websites by default to reduce breakage - Changing this setting will restart your browser</string>

    <!-- SVG -->
    <string name="preference_svg_enabled">Enable Scalable Vector Graphics (SVG)</string>

    <!-- UnifiedPush -->
    <!-- Label for enabling UnifiedPush -->
    <string name="preferences_debug_settings_unifiedpush">Enable UnifiedPush</string>
    <!-- Label for using UnifiedPush. UnifiedPush is a decentralized push notification system, so users can benefit from mobile push notifications without relying on Google Services. Users choose the application (and server) they wish to use to manage webpush for other applications. If enabled, the browser (e.g. IronFox) will receive webpush messages via the chosen app. -->
    <string name="preferences_unifiedpush">Use UnifiedPush</string>
    <!-- Toast shown after confirming the UnifiedPush configuration -->
    <string name="toast_change_unifiedpush_done">UnifiedPush setting modified. Quitting the application to apply changes…</string>

    <!-- Wallpapers -->
    <string name="wallpaper_collection_fennec" translatable="false">Fennec F-Droid</string>
    <string name="wallpaper_collection_fennec_description">Wallpapers from Fennec F-Droid. %s</string>

    <!-- WebAssembly -->
    <string name="preference_wasm_enabled">Enable WebAssembly (WASM)</string>

    <!-- WebGL -->
    <string name="preference_webgl_disabled">Disable WebGL</string>
    <string name="preference_fpp_overrides_ironfox_webgl_enabled">Enable WebGL overrides from IronFox</string>
    <string name="preference_fpp_overrides_ironfox_webgl_summary">When WebGL is disabled, re-enables WebGL for certain websites by default to reduce breakage - Changing this setting will restart your browser</string>

    <!-- WebRTC -->
    <string name="preference_webrtc_enabled">Enable WebRTC</string>

    <!-- XPInstall -->
    <string name="extension_failed_to_install_xpinstall_disabled_error">Installation of add-ons is currently disabled. If desired, at the cost of security, you can enable it by navigating to Settings -> IronFox -> IronFox settings -> Security -> Allow installation of add-ons.</string>
    <string name="preference_xpinstall_enabled">Allow installation of add-ons</string>

    <!-- Onboarding - Title and description -->
    <string name="onboarding_if_preferences_title">Customize IronFox</string>
    <string name="onboarding_if_preferences_description">Customize IronFox to suit your liking</string>

    <!-- Onboarding - DoH -->
    <string name="onboarding_if_preference_doh_caption">Secure your browsing with DNS-over-HTTPS for enhanced privacy and security.</string>
    <string name="onboarding_if_preference_doh_off_summary" translatable="false"></string>

    <!-- Onboarding - uBlock Origin -->
    <string name="onboarding_install_ublock_title">Install uBlock Origin</string>
    <string name="onboarding_install_ublock_caption">Block ads, trackers, and malware for a safer, cleaner browsing experience.</string>
    <string name="onboarding_install_ublock_description">uBlock Origin enhances your privacy and security by blocking unwanted content. IronFox uses a custom uBlock Origin configuration with curated filter lists for optimal protection and performance.</string>

    <!-- Onboarding - States -->
    <string name="onboarding_state_installing_ublock">Installing uBlock Origin…</string>
    <string name="onboarding_state_installing_ublock_error">An error occurred while installing uBlock Origin: %1$s</string>

    <!-- Onboarding - Actions -->
    <string name="onboarding_action_retry">Retry</string>

    <!-- OLED Theme -->
    <string name="preference_oled_theme" translatable="false">OLED</string>
</resources>
